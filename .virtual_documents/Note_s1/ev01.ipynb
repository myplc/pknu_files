import random as r
r.seed(302)
num = [0] * 6
# [0, 0, 0, 0, 0, 0]

turn = 10000

print("주사위 던지기 시작")
for n in range(turn):
    dice = r.randint(1,6) # 1,2,3,4,5,6
    num[dice - 1] += 1

for x in range(6):
    print(f"{x+1}번 횟수: {num[x]} / {((num[x]/turn)*100):.2f} %")


